{
  "name": "order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "weight": {
      "type": "number",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "location": {
      "type": "object"
    },
    "status": {
      "type": "string",
      "default": "pending",
      "enum": [
        "pending",
        "processing",
        "onTheWay",
        "complete",
        "canceled"
      ]
    },
    "petroGoPrice": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {  },
  "acls": [
  ],
  "methods": {}
}
